{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "LogPayload",
  "type": "object",
  "properties": {
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "message": {
      "type": "string"
    },
    "level": {
      "type": "string",
      "enum": ["info", "warning", "error"]
    }
  },
  "required": ["timestamp", "message", "level"]
}

